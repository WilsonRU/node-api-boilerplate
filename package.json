  },
  "dependencies": {
    "@prisma/client": "^3.9.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.11.11",
    "prisma": "^3.9.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
